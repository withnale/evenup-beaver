# Managed by puppet - do not modify
[beaver]
transport = redis
redis_url = <%= scope.lookupvar('beaver::redis_url') %>
redis_namespace: <%= scope.lookupvar('beaver::redis_namespace') %>
queue_timeout: <%= scope.lookupvar('beaver::queue_timeout') %>
logstash_version: 1
<% if scope.lookupvar('beaver::enable_sincedb') -%>
sincedb_path: <%= scope.lookupvar('beaver::sincedb_path') %>
<% end -%>
<% if @multiline_regex_before -%>
multiline_regex_before: <%= @multiline_regex_before %>
<% end -%>
<% if @multiline_regex_after -%>
multiline_regex_after: <%= @multiline_regex_after %>
<% end -%>
<% if scope.lookupvar('beaver::queue_timeout') != '' -%>
queue_timeout: <%= scope.lookupvar('beaver::queue_timeout') %>
<% end -%>
<% if scope.lookupvar('beaver::discover_interval') != '' -%>
discover_interval: <%= scope.lookupvar('beaver::discover_interval') %>
<% end -%>

